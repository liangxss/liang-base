<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="titleBarText" format="string"/>
    </declare-styleable>

    <declare-styleable name="DashLineView">
        <attr name="drawableStart" format="reference"/>
        <attr name="dashLineStyle" >
            <enum name="smallGray" value="0"/>
            <enum name="smallBlack" value="1"/>
            <enum name="normalGray" value="2"/>
            <enum name="normalBlack" value="3"/>
            <enum name="bigGray" value="4"/>
            <enum name="bigBlack" value="5"/>
            <enum name="hugeGray" value="6"/>
            <enum name="hugeBlack" value="7"/>
            <enum name="bigHugeGray" value="8"/>
            <enum name="bigHugeBlack" value="9"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CommonTitleView">
        <attr name="orientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>

    </declare-styleable>
</resources>
